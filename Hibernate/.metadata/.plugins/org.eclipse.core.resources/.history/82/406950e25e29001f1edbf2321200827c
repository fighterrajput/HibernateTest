package com.rays.auction;

import java.util.HashSet;
import java.util.Set;

public class TestAuction {
	public static void main(String[] args) {
		Bid bid1 = new Bid();
		bid1.setId(1);
		bid1.setAmount(1000);
		bid1.setTimeStamp("A");

		Bid bid2 = new Bid();
		bid1.setId(2);
		bid1.setAmount(2000);
		bid1.setTimeStamp("AA");

		Bid bid3 = new Bid();
		bid1.setId(3);
		bid1.setAmount(3000);
		bid1.setTimeStamp("AAA");

		Set bids = new HashSet();

		bids.add(bid1);
		bids.add(bid2);
		bids.add(bid3);
	}
}
